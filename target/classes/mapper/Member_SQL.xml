<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="memberstore">


	<select id="selectStoreList" parameterType="hashmap"
		resultType="hashmap">
		<![CDATA[
			SELECT 
			STORE, S_NAME,  ADDRESS1, ADDRESS2, S_STATUS, S_PHONE
			FROM  S_MEMBER 
		]]>
	</select>
	
		<select id="storeDetail" parameterType="hashmap"
		resultType="hashmap">
		<![CDATA[
			SELECT 
			STORE, S_NAME,  ADDRESS1, ADDRESS2, S_PHONE, S_STATUS 
			FROM  S_MEMBER 
		]]>
	</select>
	



</mapper>